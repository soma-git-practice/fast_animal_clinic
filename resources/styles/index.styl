        &::after
          content: '';
          position: absolute;
          top: 0;
          left: -85%;
          width: 80%;
          height: 100%;
          background: linear-gradient(to right, rgba(255,255,255,0) 0%, rgba(255,255,255,1) 100%);
          transform: skewX(-25deg);
        &:hover::after
          animation: shine 0.7s;
