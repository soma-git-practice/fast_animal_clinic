@charset "utf-8"

*{
  -webkit-font-smoothing: antialiased;
}

img{
  width: 100%;
}

.SPBR
  display block
  @media screen and (max-width: 768px)
    display none

// 使い回し
bg_blue = #19315A
bg_yellow = #FFE872
notosans = 'Noto Sans JP', sans-serif
notoserif = 'Noto Serif JP', serif;

// ファーストビューの画像アニメーション

@keyframes changeImgAnimation
  0%
    opacity 0
  10%
    opacity 1
  90%
    opacity 1
  100%
    opacity 0

body{
  background-color: bg_blue
}

// 救急ボタンアニメーション
@keyframes shine
  100%
    left: 125%
// お知らせボタンアニメーション
@keyframes slide1
  0%
    opacity 0
  20%
    opacity 1
  40%
    opacity 1
  60%
    opacity 0
  80%
    opacity 1
  100%
    opacity 1

@keyframes slide2
  0%
    opacity 1
  20%
    opacity 0
  40%
    opacity 1
  60%
    opacity 1
  80%
    opacity 0
  100%
    opacity 1

@keyframes slide3
  0%
    opacity 1
  20%
    opacity 1
  40%
    opacity 0
  60%
    opacity 1
  80%
    opacity 1
  100%
    opacity 0

/* Header */
.Header
  position fixed
  z-index 9999
  top 0
  width 100%
  transition: all .5s
  &.isHidden
    opacity 0
    transform: translateY(-100%);
  &__Inner
    position relative
    z-index 9999
    margin-right auto
    margin-left auto
    padding-left 20px
    display flex
    justify-content: space-between
    align-items: center
    background-color: #1C3765
    box-shadow: 1px 0px 7px #0B162B
    @media screen and (max-width: 768px)
      padding-right 20px
      padding-top 10px
      padding-bottom 10px
  &-Logo
    &-Link
      display flex
      align-items: center
      gap: 20px
      color: #FFF
      font-family: notosans
      font-size 35px
      letter-spacing: 0.05em
      font-weight: 700
      @media screen and (max-width: 1300px)
        gap 10px
        font-size 20px
      &-Img
        width 81px
        @media screen and (max-width: 1300px)
          width 41px
  &-PC
    @media screen and (max-width: 768px)
      display none
    &-Nav
      display flex
      &-Container
        @media screen and (max-width: 1300px)
          display flex
        &-Item
          position relative
          padding 25px
          display flex
          align-items: center
          justify-content: center
          height 115px
          font-family: notosans
          font-weight 500
          font-size 25px
          color: #FFF
          @media screen and (max-width: 1300px)
            padding 10px
            height 64px
            font-size 16px
            height auto
          &::before
            content: ""
            position absolute
            bottom 25%
            display block
            width 50%
            height 1px
            transition: all .3s;
            background-color: #FFF
            transform: scale(0,1)
            transform-origin: right
          &:hover.Header-PC-Nav-Container-Item::before
            transform: scale(1,1)
      &-Reserve
        margin-left 25px
        background-color: white
        @media screen and (max-width: 1300px)
          margin-left 10px
        &-Item
          position relative
          padding-right 45px
          padding-left 45px
          display flex
          align-items: center
          justify-content: center
          gap 10px
          height 115px
          font-family: notosans
          font-weight 500
          font-size 25px
          color: bg_blue
          @media screen and (max-width: 1300px)
            padding 20px
            gap 5px
            height auto
            font-size 16px
          &-Img
            width 37px
            @media screen and (max-width: 1300px)
              width 22px
          &::before
            content: ""
            position absolute
            bottom 25%
            display block
            width 50%
            height 1px
            transition: all .3s;
            background-color: bg_blue
            transform: scale(0,1)
            transform-origin: right
          &:hover.Header-PC-Nav-Reserve-Item::before
            transform: scale(1,1)
  &-Hamburger
    position relative
    display none
    width 30px
    height 30px
    @media screen and (max-width: 768px)
      display block
    &-Top
      position absolute
      top 0
      right 0
      left 0
      bottom: 0
      margin auto
      width 100%
      height 1px
      background-color: #D9D9D9
      transform: translateY(-7px)
      transition: all 0.3s
    &-Middle
      position absolute
      top 0
      right 0
      left 0
      bottom: 0
      margin auto
      width 100%
      height 1px
      background-color: #D9D9D9
      transition: all 0.3s
    &-Bottom
      position absolute
      top 0
      right 0
      left 0
      bottom: 0
      margin auto
      width 100%
      height 1px
      background-color: #D9D9D9
      transform: translateY(7px)
      transition: all 0.3s
    &.isActive
      & .Header-Hamburger-Top
        transform: translateY(0) rotate(-45deg)
      & .Header-Hamburger-Middle
        opacity 0
      & .Header-Hamburger-Bottom
        transform: translateY(0) rotate(45deg)
  &-SP
    position absolute
    z-index 8888
    padding-top 15px
    padding-bottom 15px
    display none
    align-items: center
    justify-content: center
    width 100%
    height calc(100vh - 60px)
    transition: all 0.3s
    background-color: bg_blue
    @media screen and (max-width: 768px)
      display flex
    // js操作
    &.isRight
      opacity 0
      pointer-events: none
      filter: blur(2px)
      transform: translateX(100%);
    &__Inner
      margin-right auto
      margin-left auto
      max-width 280px
    &-Date
      margin-bottom 5px
    &-Supplement
      margin-bottom 10px
    &-Bullet
      margin-bottom 15px
      display flex
      flex-wrap: wrap
      &-Item
        position relative
        display flex
        align-items: center
        justify-content: center
        width 50%
        height 50px
        text-align: center
        font-size 18px
        font-family notosans
        color: #fff
        &::before
          content ""
          position absolute
          bottom 6px
          width 36px
          height 1px
          background-color: #FFF
    &-Reserve
      margin-bottom 15px
      border-radius: 2px
      box-shadow: -2px -2px 4px 0px rgba(0, 0, 0, 0.35) inset;
      display flex
      align-items: center
      justify-content: center
      width 100%
      height 60px
      text-align: center
      background-color: #FFF
      color: #19315A;
      font-size: 20px;
      font-family: notosans;
      font-weight: 700;
      line-height: 180%;
      letter-spacing: 0.1em;
    &-Aid
      border-radius: 2px
      box-shadow: -2px -2px 4px 0px rgba(0, 0, 0, 0.35) inset;
      display flex
      align-items: center
      justify-content: center
      width 100%
      height 60px
      text-align: center
      background-color: #FFF
      color: #19315A;
      font-size: 20px;
      font-family: notosans;
      font-weight: 700;
      line-height: 180%;
      letter-spacing: 0.1em;
/* バナー */
.Banner
  position fixed
  z-index 9999
  right 0
  bottom 46px
  padding-top 41px
  padding-right 41px
  padding-bottom 38px
  display flex
  align-items: center
  border-radius: 2px 0px 0px 2px;
  border: 1px solid #1A325A;
  background: #FFF;
  box-shadow: 0px 0px 5px 0px rgba(0, 0, 0, 0.30);
  cursor: pointer
  transform: translateX(537px)
  transition: all .3s
  @media screen and (max-width: 768px)
    display none
  &.isActive
    transform: translateX(0px)
  &.isHidden
    opacity 0
    pointer-events: none
  &-Left
    padding-right 25px
    padding-left 25px
    &-Headline
      margin-bottom 20px
      writing-mode: vertical-lr
      color: #19315A;
      font-size: 20px;
      font-family: notosans
      font-weight: 500;
    &-Decoration
      position relative
      width 20px
      height 20px
      &-First
        position absolute
        top 0
        right 0
        bottom 0
        left 0
        margin auto
        width 100%
        height 2px
        background-color: #1A315A
      &-Second
        position absolute
        top 0
        right 0
        bottom 0
        left 0
        margin auto
        width 100%
        height 2px
        transform: rotate(90deg)
        background-color: #1A315A
        transition: all .3s
        &.isActive
          opacity 0
  &-Right
    width 494px
    &-Table
      margin-bottom 20px
/* ファーストビュー */
.Firstview
  position relative
  margin-top 115px
  background-color: bg_blue
  @media screen and (max-width: 1300px)
    margin-top 60px
  &-Bg
    position: absolute
    top 0
    right 0
    left 0
    margin auto
    display flex
    width 100%
    height calc(100vh - 115px)
    @media screen and (max-width: 1300px)
      height:  calc(100vh - 60px)
    &-Padding
      width 15.625%
      @media screen and (max-width: 768px)
        display none
    &-Pictures
      position relative
      width 100%
      overflow: hidden
      &-1
        position absolute
        top 0
        right 0
        bottom 0
        left 0
        margin auto
        background-position: right
        background-size: cover
        transition: all 2s
        @media screen and (max-width: 1300px)
          background-position: 69% 100%
        &.isHidden
          opacity 0
          pointer-events: none
          transform: scale(1.03)
      &-2
        position absolute
        top 0
        right 0
        bottom 0
        left 0
        margin auto
        background-position: 40%
        background-size: cover
        transition: all 2s
        &.isHidden
          opacity 0
          pointer-events: none
          transform: scale(1.03)
  &-Contents
    position relative
    margin-right auto
    margin-left auto
    padding-right 40px
    padding-bottom 83px
    padding-left 40px
    display flex
    align-items: flex-end
    flex-wrap: wrap
    width 100%
    height: calc(100vh - 115px)
    max-width 1440px
    @media screen and (max-width: 1300px)
      height:  calc(100vh - 60px)
    @media screen and (max-width: 768px)
      padding-top 20px
      padding-right 20px
      padding-bottom 20px
      padding-left 20px
      align-items: flex-start
    &-MainTitle
      margin-bottom 12px
      padding-top 20px
      padding-right 24px
      padding-bottom 20px
      padding-left 20px
      width fit-content
      color: bg_blue
      background-color: #FFF
      @media screen and (max-width: 768px)
        margin-bottom clamp(5px,calc(500vw/320),12px);
        padding-top clamp(9px,calc(900vw/320),20px);
        padding-right: clamp(7px,calc(700vw/320),24px);
        padding-bottom: clamp(9px,calc(900vw/320),20px);
        padding-left: clamp(7px,calc(700vw/320),24px);
      &-Img
        @media screen and (max-width: 1300px)
          height clamp(23px,calc(2300vw/320),45px);
    &-SubTitle
      padding-top 4px
      padding-right 20px
      padding-bottom 4px
      padding-left 20px
      display flex
      align-items: flex-end
      gap 9px
      width 100%
      font-weight 500
      font-size 30px
      font-family: notosans
      letter-spacing: 0.05em
      background-color: #142544
      color: #FFF
      @media screen and (max-width: 1300px)
        display none
      &-Big
        font-size 45px
    &-SubTitleSP
      display none
      @media screen and (max-width: 1300px)
        margin-top clamp(5px,calc(500vw/320),10px)
        padding-top: clamp(2px,calc(200vw/320),4px);
        padding-right:clamp(7px,calc(700vw/320),20px);
        padding-bottom: clamp(2px,calc(200vw/320),4px);
        padding-left: clamp(7px,calc(700vw/320),20px);
        font-size clamp(16px,calc(1600vw/320),30px)
        letter-spacing: 0.05em
        font-weight 700
        display block
        width fit-content
        font-family: notoserif
        background-color: #142544
        color: #FFF
      &-Big
        display block
        padding-top 2px
        padding-bottom: 2px
        font-size 45px
        @media screen and (max-width: 768px)
          font-size clamp(20px,calc(2000vw/320),45px)
/* 救急の方へ */
.Aid
  background-color: bg_blue
  &__Inner
    margin-right auto
    margin-left auto
    max-width 1380px
    padding-right 50px
    padding-left 50px
    height 100vh
    display flex
    align-items: center
    justify-content: center
