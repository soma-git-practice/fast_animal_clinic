    @media screen and (max-width: 768px)
      text-align: center
    &-Heading
      margin-bottom 55px
      font-family: notoserif
      font-weight 700
      font-size 60px
      color: #FFF
      @media screen and (max-width: 768px)
        margin-bottom 25px
        font-size 30px
    &-Text
      margin-bottom 70px
      letter-spacing: 0.05em
      font-weight 400
      font-family: notosans
      font-size 35px
      color: #FFF
      @media screen and (max-width: 768px)
        margin-bottom 30px
        font-size 14px
    &-Button
      position relative
      margin-right 0
      margin-left auto
      padding 8px 17px
      display block
      width fit-content
      line-height 180%
      letter-spacing: 0.1em
      font-weight: 700
      font-size 23px
      font-family: notosans
      color: bg_blue
      background-color: bg_yellow
      overflow: hidden
      @media screen and (max-width: 768px)
        margin-right auto
        margin-left auto
